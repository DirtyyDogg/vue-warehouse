{
  "name": "vue-warehouse",
  "version": "0.1.0b",
  "description": "Cross-browser storage for Vue.js",
  "main": "dist/vue-warehouse.common.js",
  "unpkg": "dist/vue-warehouse.umd.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/bazzite/vue-burger-menu.git"
  },
  "bugs": {
    "url": "http://github.com/bazzite/vue-burger-menu/issues"
  },
  "homepage": "https://www.bazzite.com/docs/vue-warehouse",
  "files": [
    "src",
    "dist",
    "nuxt"
  ],
  "scripts": {
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "npm run unit",
    "lint": "eslint --ext .js src test/unit",
    "report-coverage": "cat ./coverage/lcov.info | ./node_modules/.bin/codecov",
    "build": "npm run build:umd && npm run build:common",
    "build:umd": "cross-env NODE_ENV=production webpack --config build/webpack.config.umd.js --mode production --progress --hide-modules",
    "build:common": "cross-env NODE_ENV=production webpack --config build/webpack.config.common.js --mode production --progress --hide-modules",
    "deploy": "yarn lint && yarn test && yarn build && npm publish"
  },
  "keywords": [
    "bazzite",
    "vue-warehouse",
    "vue",
    "vue.js",
    "nuxt",
    "nuxt.js",
    "localstorage",
    "cookie",
    "cross-browser",
    "storage",
    "vue-plugin"
  ],
  "author": "Julio Marquez <j@bazzite.com>",
  "license": "MIT",
  "dependencies": {
    "store": "^2.0.12",
    "vue": "^2.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "chalk": "^2.0.1",
    "codecov": "^3.0.0",
    "codecov.io": "^0.1.6",
    "cross-env": "^5.1.4",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.4.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "jest": "^22.0.4",
    "node-notifier": "^5.1.2",
    "rimraf": "^2.6.0",
    "semver": "^5.3.0",
    "shelljs": "^0.8.1",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  }
}
